@ARTICLE{Emerson2003,
  author = {Emerson, Joseph and Weinstein, Yaakov S. and Saraceno, Marcos and
	Lloyd, Seth and Cory, David G.},
  title = {{Pseudo-Random Unitary Operators for Quantum Information Processing}},
  journal = {Science},
  year = {2003},
  volume = {302},
  pages = {2098--2100},
  abstract = {{In close analogy to the fundamental role of random numbers in classical
	information theory, random operators are a basic component of quantum
	information theory. Unfortunately, the implementation of random unitary
	operators on a quantum processor is exponentially hard. Here we introduce
	a method for generating pseudo-random unitary operators that can
	reproduce those statistical properties of random unitary operators
	most relevant to quantum information tasks. This method requires
	exponentially fewer resources, and hence enables the practical application
	of random unitary operators in quantum communication and information
	processing protocols. Using a nuclear magnetic resonance quantum
	processor, we were able to realize pseudorandom unitary operators
	that reproduce the expected random distribution of matrix elements.}},
