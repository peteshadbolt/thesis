  title = {{Entangling and Disentangling Power of Unitary Transformations Are
	Not Equal}},
  journal = {Physical Review Letters},
  year = {2009},
  volume = {103},
  pages = {030501+},
  number = {3},
  month = jul,
  __markedentry = {[pete:]},
  abstract = {{We consider two capacity quantities associated with bipartite unitary
	gates: the entangling and the disentangling power. Here, we prove
	that these capacities are different in general by constructing an
	explicit example of a qubit-qutrit unitary whose entangling power
	is maximal (2\^A ebits), but whose disentangling power is strictly
	less. A corollary is that there can be no unique ordering for unitary
	gates in terms of their ability to perform nonlocal tasks. Finally,
	we show that in large dimensions, almost all bipartite unitaries
	have entangling and disentangling capacities close to maximal.}},
  citeulike-article-id = {8219079},
  citeulike-linkout-0 = {http://dx.doi.org/10.1103/physrevlett.103.030501},
  doi = {10.1103/physrevlett.103.030501},
